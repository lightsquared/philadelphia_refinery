@article{Saif2017,
abstract = {We study experimentally fast flames and their transition to detonation in mixtures of methane, ethane, ethylene, acetylene, and propane mixtures with oxygen. Following the interaction of a detonation wave with a column of cylinders of varying blockage ratio, the experiments demonstrate that the fast flames established are Chapman-Jouguet deflagrations, in excellent agreement with the self-similar model of Radulescu et al. (2015). The experiments indicate that these Chapman-Jouguet deflagrations dynamically restructure and amplify into fewer stronger modes until the eventual transition to detonation. The transition length to a self-sustained detonation was found to correlate very well with the mixtures' sensitivity to temperature fluctuations, reflected by the {\$}\backslashchi{\$} parameter introduced by Radulescu, which is the product of the non-dimensional activation energy {\$}E{\_}a/RT{\$} and the ratio of chemical induction to reaction time {\$}t{\_}i/t{\_}r{\$}. Correlation of the measured DDT lengths determined that the relevant characteristic time scale from chemical kinetics controlling DDT is the energy release or excitation time {\$}t{\_}r{\$}. Correlations with the cell size also capture the dependence of the DDT length on {\$}\backslashchi{\$} for fixed blockage ratios.},
archivePrefix = {arXiv},
arxivId = {1512.02554},
author = {Saif, Mohamed and Wang, Wentian and Pekalski, Andrzej and Levin, Marc and Radulescu, Matei I.},
doi = {10.1016/j.proci.2016.07.122},
eprint = {1512.02554},
file = {:Users/skmcneill/Documents/Mendeley Desktop/Saif et al/Proceedings of the Combustion Institute/Saif et al. - 2017 - Chapman-Jouguet deflagrations and their transition to detonation.pdf:pdf},
issn = {15407489},
journal = {Proceedings of the Combustion Institute},
keywords = {Chapman-Jouguet deflagrations,Deflagration-to-detonation transition,Fast flame,Supersonic combustion,propane},
mendeley-tags = {propane},
number = {2},
pages = {2771--2779},
title = {{Chapman-Jouguet deflagrations and their transition to detonation}},
volume = {36},
