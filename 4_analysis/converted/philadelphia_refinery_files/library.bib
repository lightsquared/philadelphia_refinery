@techreport{VanBavel2011,
abstract = {Director Ammunition and Explosives Regulation (DAER) requires a quantitative risk anal- ysis to support decisions to grant risk-based licenses to munitions facilities. The software application that provides the required analysis is the Safety Assessment For Explosives Risk (SAFER). DAER requested Directorate Materiel Group Operational Research team to research and analyze two issues, which were the practical interpretation of the uncer- tainty of SAFER's risk assessment, and the accuracy of SAFER's risk assessment. SAFER transparently handles uncertainties and rigorously shows how they affect SAFER's risk estimator, which is the expected number of fatalities per year. However, investigative cal- culations revealed two important sources of bias: (1) the use of numerical proxies, and (2) the assumption that the conditional probability of fatality has a lognormal distribution. The elimination of these sources of bias is recommended, thereby reducing the likelihood that a risk-based license will be rejected because of an inadvertent bias in SAFER's risk estimator.},
author = {{Van Bavel}, Gregory H},
file = {:Users/skmcneill/Documents/Mendeley Desktop/Van Bavel/Unknown/Van Bavel - 2011 - Defence R{\&}D Canada Uncertainty and the Safety Assessment For Explosives Risk (SAFER).pdf:pdf},
institution = {Defence R{\&}D Canada},
keywords = {risk,sigma},
mendeley-tags = {risk,sigma},
title = {{Defence R{\&}D Canada Uncertainty and the Safety Assessment For Explosives Risk (SAFER)}},
year = {2011}
}
@article{Monsfield2001,
abstract = {Six test firings of large improvised explosive devices were carried out. The principal objectives of the firings were to measure the physical effects of the explosions upon representative objects placed nearby and to recover any chemical traces deposited on these objects. The results are intended for use as an aid in determining the approximate size and type of an explosive employed in terrorist attacks. Three 454 kg charges of a mixture of calcium ammonium nitrate (CAN) fertilizer and sugar, and three 2268 kg charges of a similar mixture, all confined in cylindrical steel containers were fired. Each charge was surrounded by 19 road signs mounted on posts and four vehicles, to act as witness materials. The analysis of aqueous swab extracts taken from the witness materials after firing showed the recovery of nitrate, ammonium and low levels of glucose. No sucrose was detected. Nitrate was usually recovered in greater quantities than ammonium and recovery generally decreased with increasing distance from the charges in any given direction. Quantities recovered from objects placed at the same distance in different directions varied considerably. Patterns of physical damage to the witness materials could be discerned according to their distance from the charge and the size of the charge. The velocities of detonation and air blast effects were measured.},
author = {Phillips, Susan A. and Lowe, Alison and Marshall, Maurice and Hubbard, Peter and Burmeister, Steven G. and Williams, David R.},
doi = {10.1520/JFS14685J},
file = {:Users/skmcneill/Documents/Mendeley Desktop/Phillips et al/Journal of Forensic Sciences/Phillips et al. - 2000 - Physical and Chemical Evidence Remaining After the Explosion of Large Improvised Bombs. Part 1 Firings of Ammon.pdf:pdf},
issn = {00221198},
journal = {Journal of Forensic Sciences},
keywords = {ammonium nitrate,chemical,fer-,forensic science,improvised explosive device,physical damage,residues,sugar,tilizer,tnt equiva-,velocity of detonation,witness materials},
month = {mar},
number = {2},
pages = {14685J},
pmid = {10782953},
title = {{Physical and Chemical Evidence Remaining After the Explosion of Large Improvised Bombs. Part 1: Firings of Ammonium Nitrate/Sugar and Urea Nitrate}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/10782953 http://www.astm.org/doiLink.cgi?JFS14685J},
volume = {45},
year = {2000}
}
