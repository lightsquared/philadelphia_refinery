@misc{Ermolaev2010,
abstract = {The explosive and deflagration properties of black powder differ significantly from those of modern propellants and compositions based on ammonium nitrate or ammonium perchlorate. Possessing a high combustibility, black powder is capable of maintaining stable combustion at high velocities in various shells, be it steel shells or thin-walled plastic tubes, without experiencing deflagration-to-detonation transition. It is extremely difficult to detonate black powder, even using a powerful booster detonator. The results of numerical simulations of a number of key experiments on the convective combustion and shock initiation of black powder described in the literature are presented. The calculations were performed within the framework of a model developed previously for describing the convective combustion of granulated pyroxylin powders, with small modifications being introduced to allow for the specific properties of black powder. The thermophysical properties of the products of combustion and detonation and the parameters of the equation of state of black powder were determined from thermodynamic calculations. The calculation results were found to be in close agreement with the experimental data. The simulation results were used to analyze the regularities of the wave processes in the system and their relation to the properties of black powder and the experimental conditions. It was demonstrated that the effects observed could be explained by a weak dependence of the burning rate of black powder on the pressure.},
author = {Ermolaev, B. S. and Belyaev, A. A. and Viktorov, S. B. and Sleptsov, K. A. and Zharikova, S. Yu.},
booktitle = {Russian Journal of Physical Chemistry B},
doi = {10.1134/s1990793110030103},
file = {:Users/skmcneill/Documents/Mendeley Desktop/Ermolaev et al/Russian Journal of Physical Chemistry B/Ermolaev et al. - 2010 - Nonideal regimes of deflagration and detonation of black powder.pdf:pdf},
isbn = {1990-7931},
issn = {1990-7931},
month = {jun},
number = {3},
pages = {428--439},
publisher = {SP MAIK Nauka/Interperiodica},
title = {{Nonideal regimes of deflagration and detonation of black powder}},
url = {http://link.springer.com/10.1134/S1990793110030103},
volume = {4},
year = {2010}
}
@article{Cooper2008,
abstract = {The term ''TNT Equivalence'' is used throughout the explosives and$\backslash$nrelated industries to compare the effects of the output of a given$\backslash$nexplosive to that of TNT. This is done for technical design reasons$\backslash$nin scaling calculation such as for the prediction of blast waves,$\backslash$ncraters, and structural response, and is also used as a basis for$\backslash$ngovernment regulations controlling the shipping, handling and storage$\backslash$nof explosive materials, as well as for the siting and design of explosive$\backslash$nfacilities. TNT equivalence is determined experimentally by several$\backslash$ndifferent types of tests, the most common of which include: plate$\backslash$ndent, ballistic mortar, trauzl, sand crush, and air blast. All of$\backslash$nthese tests do not necessarily measure the same output property of$\backslash$nthe sample explosive. As examples of this, some tests depend simply$\backslash$nupon the CJ pressure, some depend upon the PV work in the CJ zone$\backslash$nand in the Taylor wave behind the CJ plane, some are functions of$\backslash$nthe total work which includes that from secondary combustion in the$\backslash$nair mixing region of the fireball and are acutely effected by the$\backslash$nshape of the pressure-time profile of the wave. Some of the tests$\backslash$nincorporate systematic errors which are not readily apparent, and$\backslash$nwhich have a profound effect upon skewing the resultant data. Further,$\backslash$nsome of the tests produce different TNT Equivalents for the same$\backslash$nexplosive which are a function of the conditions at which the test$\backslash$nis run. This paper describes the various tests used, discusses the$\backslash$nresults of each test and makes detailed commentary on what the test$\backslash$nis actually measuring, how the results may be interpreted, and if$\backslash$nand how these results can be predicted by first principals based$\backslash$ncalculations. Extensive data bases are referred to throughout the$\backslash$npaper and used in examples for each point in the commentaries.},
author = {Cooper, P W},
