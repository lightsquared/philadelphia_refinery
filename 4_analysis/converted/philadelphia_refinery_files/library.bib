@article{DiazAlonso2006,
abstract = {A number of models have been proposed to calculate overpressure and impulse from accidental industrial explosions. When the blast is produced by explosives, pyrotechnics or unstable substances, the TNT equivalent model is widely used. From the curves given by this model, data are fitted to obtain equations showing the relationship between overpressure, impulse and distance. These equations, referred to here as characteristic curves, can be fitted by means of power equations, which depend on the TNT equivalent mass. Characteristic curves allow determination of overpressure and impulse at each distance. ?? 2006 Elsevier Ltd. All rights reserved.},
