}
@inproceedings{Chen2011,
abstract = {The current CCPS/TNO's BLEVE (Boiling Liquid Expanding Vapor Explosion) overpressure methodology is largely derived from that published by Baker in 1975{\textless}sup{\textgreater}[1]{\textless}/sup{\textgreater}. This methodology calculates the explosion energy using an isentropic expansion assumption and Baker's graphical method. The overpressure is determined graphically assuming similarity between BLEVEs and high-explosives. Baker's work was based on limited data on vapor-filled vessels and the accuracy of his method for BLEVE overpressure calculations was not tested against experimental data due to the lack of data available. Many experimental studies on BLEVE overpressure (Giesbrecht{\textless}sup{\textgreater}[2]{\textless}/sup{\textgreater}, Birk {\textless}sup{\textgreater}[3-5]{\textless}/sup{\textgreater}, Bulke{\textless}sup{\textgreater}[6]{\textless}/sup{\textgreater}, etc.) have been published since the early 80's. We have carried out a comprehensive review and found over 100 experimental data points from the existing literature. Based on a comparison of overpressure calculations to the literature data, we found that the CCPS/TNO's method over-predicts BLEVE side-on overpressures in the range of typical industrial application (1{\textless}R{\textless}10, and 1{\textless}P{\textless}inf{\textgreater}so{\textless}/inf{\textgreater}{\textless}100kpa), especially in the near field (R{\textless}4). We also found that for liquid-full vessels, the current method underestimates the overpressure in certain regions. Starting with Baker's algorithm, a new, more accurate methodology for BLEVE over-pressure prediction was developed. Based on recent developments in the literature, adiabatic irreversible expansion was assumed in calculating explosion energies. Baker's correlation was adjusted with new multipliers derived to ensure that no experimental overpressures are under-predicted. Different sets of multipliers were determined for both BLEVE and liquid-full cases. Overall, the new method is 10-50{\%} less conservative than the CCPS/TNO's method.},
author = {Chen, Fei and Lutostansky, Elizabeth and Schork, Joan},
booktitle = {2011 Spring Meeting {\&} 7th Global Congress on Process Safety},
keywords = {boiling liquid expanding vapor explosion (BLEVE),historical data,overpressure},
title = {{Improvement of BLEVE overpressure method using historical data}},
year = {2011}
}
