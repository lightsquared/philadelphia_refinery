isbn = {02712091 10970363},
issn = {02712091},
journal = {International Journal for Numerical Methods in Fluids},
keywords = {cardiovascular fluid mechanics,cerebral aneurysms,elastic material,fluid,hyper,space,structure interactions,time methods},
number = {October 2007},
